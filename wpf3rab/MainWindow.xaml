<Window x:Class="wpf3rab.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:wpf3rab"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Icon="Resources/3986752-building-factory-industry-icon_112345.ico">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="60"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" Background="LightGray">
            <Image Width="45" Height="45" HorizontalAlignment="Left" Margin="5" Source="Resources/zav.png"/>
            <Button x:Name="btnBlack" HorizontalAlignment="Right" Content="Назад" Click="btnBlack_Click"/>
        </Grid>
        <Frame x:Name="FrmMain" Grid.Row="1" NavigationUIVisibility="Hidden" ContentRendered="FrmMain_ContentRendered"/>
    </Grid>
</Window>
